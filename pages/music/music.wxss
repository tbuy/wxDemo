.music {
  padding: 0;
  position: relative;
}

/* 头部 */
.tab {
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
}
.tabItem {
  text-align: center;
  width: 25%;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: 6rpx solid #3C3633;
}

/* 底部 */
.player {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 140rpx;
  background-color: #C1B4AD;
}

/* 第一页 */
.musicInfo {
  color: red;
}


